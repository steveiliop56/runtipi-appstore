{
  "$schema": "https://schemas.runtipi.io/v2/dynamic-compose.json",
  "schemaVersion": 2,
  "services": [
    {
      "name": "termix",
      "image": "ghcr.io/lukegus/termix:release-1.8.0",
      "isMain": true,
      "internalPort": 8080,
      "volumes": [
        {
          "containerPath": "/app/data",
          "hostPath": "${APP_DATA_DIR}/data"
        }
      ],
      "environment": [
        {
          "key": "PORT",
          "value": "8080"
        }
      ]
    }
  ]
}
