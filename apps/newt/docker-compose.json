{
  "$schema": "https://schemas.runtipi.io/v2/dynamic-compose.json",
  "services": [
    {
      "name": "newt",
      "image": "fosrl/newt:1.9.1",
      "environment": [
        {
          "key": "PANGOLIN_ENDPOINT",
          "value": "${PANGOLIN_ENDPOINT}"
        },
        {
          "key": "NEWT_ID",
          "value": "${NEWT_ID}"
        },
        {
          "key": "NEWT_SECRET",
          "value": "${NEWT_SECRET}"
        }
      ]
    }
  ],
  "schemaVersion": 2
}
